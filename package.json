{
  "name": "Jessie",
  "version": "0.0.17",
  "repository": "https://github.com/david-mark/jessie.git",
  "description": "Jessie Builder",
  "scripts": {
    "start": "node builder/app.js",
    "local": "nodemon src/builder/app.js",
    "dist": "npm shrinkwrap && grunt",
    "deploy": "cd dist && npm install --production; haikro build deploy --app jessie --heroku-token `heroku auth:token`",
    "runDist": "npm run dist && cd dist && npm install --production && node builder/app.js",
    "build": "npm run dist && grunt compress",
    "clean": "grunt clean",
    "app": "node src/builder/app.js"
  },
  "dependencies": {
    "body-parser": "^1.12.4",
    "express": "^4.0.0",
    "simplesets": "1.2.0",
    "swig": "^1.4.2",
    "swig-extras": "0.0.1",
    "uglify-js": "1.3.3"
  },
  "devDependencies": {
    "commander": "1.1.1",
    "grunt": "^0.4.5",
    "grunt-contrib-clean": "^0.6.0",
    "grunt-contrib-compress": "^0.13.0",
    "grunt-contrib-concat": "^0.5.1",
    "grunt-contrib-copy": "^0.8.0",
    "grunt-contrib-cssmin": "^0.12.3",
    "grunt-contrib-uglify": "^0.9.1",
    "grunt-filerev": "^2.3.1",
    "grunt-usemin": "^3.0.0",
    "haikro": "matthew-andrews/haikro#silver",
    "load-grunt-tasks": "^3.2.0",
    "nodemon": "^1.3.8"
  }
}
